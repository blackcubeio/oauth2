{
    "name": "blackcube/oauth2",
    "description": "OAuth2/JWT toolbox with multi-population support",
    "keywords": [
        "oauth2",
        "jwt",
        "authentication",
        "multi-tenant"
    ],
    "type": "library",
    "license": "BSD-3-Clause",
    "homepage": "https://github.com/blackcubeio/oauth2",
    "support": {
        "issues": "https://github.com/blackcubeio/oauth2/issues",
        "source": "https://github.com/blackcubeio/oauth2"
    },
    "authors": [
        {
            "name": "Philippe Gaultier",
            "email": "philippe@blackcube.io"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=8.3",
        "bshaffer/oauth2-server-php": "^v1.14",
        "lcobucci/jwt": "^5.6",
        "psr/http-message": "^2.0",
        "psr/http-server-handler": "^1.0",
        "psr/http-server-middleware": "^1.0",
        "psr/clock": "^1.0"
    },
    "require-dev": {
        "codeception/codeception": "^5.3",
        "codeception/module-asserts": "^3.3",
        "codeception/module-db": "^3.2",
        "vlucas/phpdotenv": "^5.6",
        "yiisoft/active-record": "^1.0",
        "yiisoft/db": "^2.0",
        "yiisoft/db-migration": "^2.0",
        "yiisoft/db-mysql": "^2.0",
        "yiisoft/cache-file": "^3.2",
        "yiisoft/di": "^1.4",
        "yiisoft/factory": "^1.3",
        "yiisoft/test-support": "^3.2",
        "httpsoft/http-message": "^1.1"
    },
    "autoload": {
        "psr-4": {
            "Blackcube\\Oauth2\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Blackcube\\Oauth2\\Tests\\": "tests",
            "Blackcube\\Oauth2\\Tests\\Support\\": "tests/Support"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-devel": "1.x-dev"
        },
        "config-plugin-file": "config/configuration.php"
    },
    "config": {
        "sort-packages": true
    }
}
